[
    {
      "essential": true,
      "image": "${image_url}",
      "name": "${container_name}",
      "portMappings": [
        {
          "containerPort": 8888,
          "hostPort": 8888
        },
        {
            "containerPort": 25,
            "hostPort": 25
        }
      ],
      "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
              "awslogs-group": "${log_group_name}",
              "awslogs-region": "${region}",
              "awslogs-stream-prefix": "ecs"
          }
      },
      "secrets": [
        {
            "name": "CONFIG_SERVICE_API_KEY",
            "valueFrom": "arn:aws:ssm:${region}:${account}:parameter/oce/${environment}/ics/CONFIG_SERVICE_API_KEY"
        },
        {
            "name": "CRYPTO_KEY",
            "valueFrom": "arn:aws:ssm:${region}:${account}:parameter/oce/${environment}/ics/CRYPTO_KEY"
        },
        {
            "name": "DB_PASSWORD",
            "valueFrom": "arn:aws:ssm:${region}:${account}:parameter/oce/${environment}/ics/DB_PASSWORD"
        },
        {
            "name": "DB_TYPE",
            "valueFrom": "arn:aws:ssm:${region}:${account}:parameter/oce/${environment}/ics/DB_TYPE"
        },
        {
            "name": "DB_HOST",
            "valueFrom": "arn:aws:ssm:${region}:${account}:parameter/oce/${environment}/ics/DB_HOST"
        },
        {
            "name": "DB_NAME",
            "valueFrom": "arn:aws:ssm:${region}:${account}:parameter/oce/${environment}/ics/DB_NAME"
        },
        {
            "name": "DB_USER",
            "valueFrom": "arn:aws:ssm:${region}:${account}:parameter/oce/${environment}/ics/DB_USER"
        },
        {
            "name": "DB_SSL",
            "valueFrom": "arn:aws:ssm:${region}:${account}:parameter/oce/${environment}/ics/DB_SSL"
        },
        {
            "name": "SFAPI_ENDPOINT",
            "valueFrom": "arn:aws:ssm:${region}:${account}:parameter/oce/${environment}/ics/SFAPI_ENDPOINT"
        },
        {
            "name": "CONFIG_SERVICE_URL",
            "valueFrom": "arn:aws:ssm:${region}:${account}:parameter/oce/${environment}/ics/CONFIG_SERVICE_URL"
        },
        {
            "name": "CRYPTO_ALGORITHM",
            "valueFrom": "arn:aws:ssm:${region}:${account}:parameter/oce/${environment}/ics/CRYPTO_ALGORITHM"
        }
      ]
    }
]